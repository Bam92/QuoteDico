{% extends "layout.html.twig" %}

{% block title %}QuoteDico - Home{% endblock %}

{% block content %}
{% for quote in quotes %}

<article>
    <p><blockquote>{{ quote.summary }}<br>
        <small>
          {{ quote.author.fName }} {{ quote.author.mName }} {{ quote.author.lName }}
        </small>
      </blockquote>
    </p>
</article>

{% endfor %}
{% endblock %}
